    "vant": "^2.5.9",
    "vue": "^2.6.11",
    "vue-baidu-map": "^0.21.22",
    "vue-jsonp": "^0.1.8",
    "vue-router": "^3.1.6",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.1.3",
    "weixin-js-sdk": "^1.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
